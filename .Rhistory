library(tidyverse)
SJM0 <- read_csv("DADA/SHAPE JOY - Module 0/SHAPEJOY_Module0Data-copy.csv") %>%
mutate(week = 0)
SJM1 <- read_csv("DADA/SHAPE JOY - Module 1/SJ Mod1.copy.csv")%>%
mutate(week = 1)
SJM2 <- read_csv("DADA/SHAPE JOY - Module 2/SJMod2copy.csv")%>%
mutate(week = 2)
SJM3 <- read_csv("DADA/SHAPE JOY - Module 3/SJ Mod3copy.csv")%>%
mutate(week = 3)
SJM4 <- read_csv("DADA/SHAPE JOY - Module 4/SJMOD4copy.csv")%>%
mutate(week = 4)
SJM5 <- read_csv("DADA/SHAPE JOY - Module 5/SJ Mod5.copy.csv")%>%
mutate(week = 5)
SJMAD <- read_csv("DADA/AllDataOverTime.csv")
library(tidyr)
library(ggplot2)
#called binded data to be referenced in graphs diddle
diddle <- bind_rows(SJM0, SJM1, SJM2, SJM3, SJM4, SJM5) %>%
filter((!ID %in% c(506, 510, 513, 514)))
# create a lookup table for unique ID values and a new anonymous ID
lookup_table <- diddle %>%
distinct(ID) %>%
mutate(position = row_number()) %>%
mutate(participant_id = letters[position])
# join lookup table to diddle
anon_diddle <- left_join(diddle, lookup_table) %>%
mutate(ID = participant_id) %>%
select(-position, -participant_id)
ggplot(data=anon_diddle, aes(x=week, y=MindfulParenting_A_1, color = as.factor(ID)))+
geom_line() +
geom_point() +
facet_wrap("ID", ncol =3)+
theme_bw()+
theme(legend.position ="none") +
labs(x = "Week Number", y = "Reported Value",
title = "Mastery Mindful Parenting")
# make into long format  where there is one column for all of the different
# measures and the other column is the corresponding values
long_diddle <- anon_diddle %>%
pivot_longer(cols  = -c(ID, DOB, week), names_to = "measure")
# add labels to long_diddle
measure_labels <- c(
"Mastery Emotion Coaching",
"Frequency Emotion Coaching",
"Mastery Positive Empathy",
"Frequency Positive Empathy",
"Mastery Mindful Parenting",
"Frequency Mindful Parenting",
"Mastery Pro-social",
"Frequency Pro-social"
)
measure_lookup <- long_diddle %>%
distinct(measure) %>%
arrange(measure) %>%
mutate(measure_label = measure_labels)
diddle_labels <- left_join(long_diddle, measure_lookup)
# making a plot
ggplot(long_diddle, aes(value, measure)) +
geom_boxplot() +
theme_bw()
# we filter the long data set to the following measures
diddle_labels %>%
filter(
measure %in% c("EmpathyforPos_O_1", "ProsocialPos_O_1", "EmotionCoaching_A_1", "MindfulParenting_A_1")
) %>%
# make a box plot
ggplot(aes(value, measure_label)) +
geom_boxplot() +
# add the dots on top of the box plot
ggbeeswarm::geom_beeswarm(alpha = 0.5) +
# make a small plot for each week
facet_wrap("week")
ggplot(data=diddle, aes(x=week, y=ProsocialPos_A_1, color = as.factor(ID))) +
geom_boxplot(width = 0.1, outlier.colour = "blue")
library(dplyr)
week_summary <- diddle_labels %>%
group_by(week, measure_label) %>%
summarise(
avg_val = mean(value),
sd = sd(value, na.rm = TRUE)
) %>% ungroup()
week_summary %>%
ggplot(aes(week, avg_val)) +
geom_line() +
geom_point() +
facet_wrap("measure_label", ncol = 2) +
theme_pubclean() +
labs(x = "Week Number", y = "Mean Reported Value",
title = "Average Reported Measure by Week (Figure 3)") +
lims(y = c(0, 100))
